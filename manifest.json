{
  "name": "Vertical Rhythm Ruler",
  "version": "1.0",
  "icons": {
    "32": "icon128.png"
  },
  "permissions": [
    "tabs", "http://*/*", "https://*/*", "storage"
  ],
  "content_scripts": [{
    "matches": ["http://*/*"],
    "js": ["page.js"]
  }],
  "browser_action": {
      "default_title": "Set a vertical ruler.",
      "default_icon": "icon.png",
      "default_popup": "popup.html"
  },
  "manifest_version": 2
}
